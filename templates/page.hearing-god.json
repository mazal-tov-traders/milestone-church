        "3752b7af-9891-407b-98f2-a32fc7eec45c": {
          "type": "resources-card",
          "settings": {
            "resources_image": "shopify:\/\/shop_images\/Hearing_God_Leader_Guide.jpg",
            "heading": "Leader Guide",
            "content": "This guide has everything you need to lead your Small Group. It includes tips on what to communicate, how to pray, what to expect, and Small Group discussion questions. The leader guide is available for mobile download, or you can pick them up at your campus.",
            "button_label": "Download Guide",
