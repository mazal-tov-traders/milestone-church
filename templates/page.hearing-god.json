    "e5846c80-b62d-473b-97f4-02ca551903c8": {
      "type": "additional-resources",
      "blocks": {
        "d58c376f-d13e-4004-af09-5cfde2ade2bc": {
          "type": "resources-card",
          "settings": {
            "resources_image": "shopify:\/\/shop_images\/Rectangle_4133724_2.jpg",
            "heading": "Weekly Videos",
            "content": "Pastor Jeff visited several of the locations where Jesus performed miracles and dives deeper into their significance through weekly videos that coordinate with the chapters in the book. ",
            "button_label": "Watch Videos",
            "button_link": "shopify:\/\/pages\/weekly-videos"
          }
        },
        "3752b7af-9891-407b-98f2-a32fc7eec45c": {
          "type": "resources-card",
          "settings": {
            "resources_image": "shopify:\/\/shop_images\/Rectangle_4133724_3.jpg",
            "heading": "Leader Guide",
            "content": "This guide has everything you need to lead your Small Group. It includes tips on what to communicate, how to pray, what to expect, and Small Group discussion questions. The leader guide is available for mobile download, or you can pick them up at your campus.",
            "button_label": "Download Guide",
            "button_link": "https:\/\/cdn.shopify.com\/s\/files\/1\/0795\/8531\/3044\/files\/Miracles_Leader_Guide.pdf?v=1702906429"
          }
        },
        "796f817e-6f8a-4489-ab65-66d16f1ce2f6": {
          "type": "resources-card",
          "settings": {
            "resources_image": "shopify:\/\/shop_images\/Rectangle_4133724_4.jpg",
            "heading": "Milestone Kid’s Resource",
            "content": "This resource was designed to help your kid grow in God’s word and is a great way to bring them along in the Hearing God series. Each week features a scripture reading, an application activity, and a prayer.",
            "button_label": "Download Here",
            "button_link": "#https:\/\/cdn.shopify.com\/s\/files\/1\/0795\/8531\/3044\/files\/Miracles_KidsResource_final_web.pdf?v=1702907107"
          }
        }
      },
